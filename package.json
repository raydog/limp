{
  "name": "limp",
  "version": "1.0.0-pre",
  "description": "An asyncronous control flow library",
  "main": "limp.js",
  "scripts": {
    "test": "mocha --check-leaks --no-exit",
    "nyan": "mocha --check-leaks -R nyan",
    "coverage": "istanbul cover _mocha -- --check-leaks --no-exit",
    "coveralls": "istanbul cover _mocha --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "limp"
  ],
  "dependencies": {
    "setimmediate": "1.0.5"
  },
  "devDependencies": {
    "bluebird": "3.4.6",
    "coveralls": "2.11.15",
    "es6-promise": "4.0.5",
    "expect": "1.20.2",
    "istanbul": "0.4.5",
    "mocha": "3.1.2",
    "mpromise": "0.5.5",
    "promise": "7.1.1",
    "q": "1.4.1",
    "rsvp": "3.3.3"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/raydog/limp.git"
  },
  "author": "Ray Myers",
